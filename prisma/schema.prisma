// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./elsydeon.db"
}

model Quote {
  id        String @id @default(uuid())
  timestamp String @default(dbgenerated("strftime('%s', 'now')"))
  quotee    String
  quoter    String
  text      String
  year      String
}
